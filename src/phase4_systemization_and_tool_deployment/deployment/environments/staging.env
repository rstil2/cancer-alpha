# Staging Environment Configuration
ENVIRONMENT=staging
API_BASE_URL=https://staging-api.cancer-alpha.com
WEB_APP_URL=https://staging.cancer-alpha.com
REDIS_URL=redis://redis:6379
DATABASE_URL=postgresql://cancer_alpha:${DB_PASSWORD}@postgres:5432/cancer_alpha_staging

# Model Configuration
MODEL_PATH=/app/models/
ENABLE_MOCK_DATA=false

# Logging
LOG_LEVEL=DEBUG
LOG_FILE=/var/log/cancer_alpha_staging.log

# Security (Use environment variables or secrets management)
JWT_SECRET=${JWT_SECRET}
API_KEY=${API_KEY}

# Performance
CACHE_TTL=1800
MAX_CONCURRENT_REQUESTS=50

# SSL/TLS
SSL_CERT_PATH=/etc/ssl/certs/cancer-alpha-staging.crt
SSL_KEY_PATH=/etc/ssl/private/cancer-alpha-staging.key

# Monitoring
PROMETHEUS_ENABLED=true
GRAFANA_ENABLED=true
HEALTH_CHECK_INTERVAL=60
